							PaddingBottom = UDim.new(0, 1),
							Parent = holder,
							Name = "",
						})
					-- 
				-- 

				-- Functions 
					function cfg.render_option(text) 
						local rendered_text = library:create("TextButton", {
							FontFace = library.font,
							TextColor3 = rgb(74, 74, 74),
							AutoButtonColor = false,
							BorderColor3 = rgb(0, 0, 0),
							Text = text,
							Size = dim2(1, -4, 0, 7),
							Position = dim2(0, 4, 0, 0),
							BackgroundTransparency = 1,
							TextXAlignment = Enum.TextXAlignment.Left,
							BorderSizePixel = 0,
							RichText = true,
							TextSize = 12,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = holder_inline,
							Name = "",
						}) library:apply_theme(rendered_text, "accent", "TextColor3")

						rendered_text.MouseEnter:Connect(function() 
							if find(flags[cfg.flag], rendered_text.Text) then 
								return 
							end 

							library:tween(rendered_text, {
								TextColor3 = rgb(150, 150, 150), 
							})
						end)

						rendered_text.MouseLeave:Connect(function() 
							if find(flags[cfg.flag], rendered_text.Text) then 
								return 
							end 

							library:tween(rendered_text, {
								TextColor3 = rgb(74, 74, 74), 
							})
						end)

						return rendered_text
					end 

					function cfg.set_visible(bool) 
						holder.Visible = bool
						holder.Size = dim_offset(dropdown_outline.AbsoluteSize.X, 0)
						holder.Position = dim_offset(dropdown_outline.AbsolutePosition.X, dropdown_outline.AbsolutePosition.Y + dropdown_outline.AbsoluteSize.Y + 60)

						library:tween(grid, {
							ImageColor3 = bool and rgb(221, 221, 221) or rgb(74, 74, 74), 
						})

						library:tween(text, {
							TextColor3 = bool and rgb(221, 221, 221) or rgb(74, 74, 74), 
						})

						library:tween(inner_text, {
							TextColor3 = bool and rgb(221, 221, 221) or rgb(74, 74, 74),
						})

						library.current = cfg
					end

					function cfg.set(value) 
						local selected = {}
						local isTable = type(value) == "table"
						
						for _, option in next, cfg.option_instances do 
							if option.Text == value or (isTable and find(value, option.Text)) then 
								insert(selected, option.Text)
								cfg.multi_items = selected
								option.TextColor3 = themes.preset.accent
							else 
								option.TextColor3 = rgb(74, 74, 74)
							end
						end

						inner_text.Text = if isTable then concat(selected, ", ") else selected[1]
						flags[cfg.flag] = selected

						cfg.callback(isTable and flags[cfg.flag] or flags[cfg.flag][1]) 
					end

					function cfg.refresh_options(list) 
						for _, option in next, cfg.option_instances do 
							option:Destroy() 
						end

						cfg.option_instances = {} 

						for _, option in next, list do 
							local new_option = cfg.render_option(option)
							insert(cfg.option_instances, new_option)

							new_option.MouseButton1Down:Connect(function()
								if cfg.multi then 
									local selected_index = find(cfg.multi_items, new_option.Text)

									if selected_index then 
										remove(cfg.multi_items, selected_index)
									else
										insert(cfg.multi_items, new_option.Text)
									end

									cfg.set(cfg.multi_items) 				
								else 
									cfg.set_visible(false)
									cfg.open = false 

									cfg.set(new_option.Text)
								end
							end)
						end
					end

					cfg.refresh_options(cfg.items)

					cfg.set(cfg.default)

					config_flags[cfg.flag] = cfg.set
				-- 

				-- Connections 
					dropdown_outline.MouseButton1Click:Connect(function()
						cfg.open = not cfg.open 

						cfg.set_visible(cfg.open)
					end)

					dropdown_outline.MouseEnter:Connect(function() 
						if cfg.open then
							return 
						end 

						library:tween(text, {
							TextColor3 = rgb(150, 150, 150), 
						})

						library:tween(inner_text, {
							TextColor3 = rgb(150, 150, 150), 
						})
					end)

					dropdown_outline.MouseLeave:Connect(function() 
						if cfg.open then
							return 
						end

						library:tween(text, {
							TextColor3 = rgb(74, 74, 74), 
						})

						library:tween(inner_text, {
							TextColor3 = rgb(74, 74, 74), 
						})
					end)
				-- 

				return setmetatable(cfg, library)
			end 

			function library:colorpicker(options) 
				local cfg = {
					name = options.name or "Color", 
					flag = options.flag or tostring(2^789),
			
					color = options.color or color(1, 1, 1), -- Default to white color if not provided
					alpha = options.alpha or 1,
			
					open = false, 
						
					callback = options.callback or function() end,
				}
			
				flags[cfg.flag] = {} 
			
				-- Instances
					-- Button 
						local gear_icon = library:create("ImageButton", {
							ImageColor3 = rgb(74, 74, 74),
							Image = "rbxassetid://81593156472608",
							BackgroundTransparency = 1,
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(0, 9, 0, 9),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = self.right_components,
							Name = "",
						})                    
					-- 
			
					-- Colorpicker Instance 
						local colorpicker_outline = library:create("Frame", {
							Parent = library.gui;
							Visible = false;
							Size = dim2(0, 161, 0, 180);
							Position = dim2(0.15710465610027313, 0, 0.4614427983760834, 0);
							BorderColor3 = rgb(0, 0, 0);
							ZIndex = 100;
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(0, 0, 0)
						});
						
						library:create("UICorner", {
							Parent = colorpicker_outline;
							CornerRadius = dim(0, 4)
						});
						
						local colorpicker_inline = library:create("Frame", {
							Parent = colorpicker_outline;
							Size = dim2(1, -2, 1, -2);
							ClipsDescendants = true;
							BorderColor3 = rgb(0, 0, 0);
							Position = dim2(0, 1, 0, 1);
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(32, 32, 32)
						});
						
