			end 

			function library:slider(options) 
				local cfg = {
					name = options.name or nil,
					suffix = options.suffix or "",
					flag = options.flag or options.name or tostring(2^789),
					callback = options.callback or function() end, 

					min = options.min or options.minimum or 0,
					max = options.max or options.maximum or 100,
					intervals = options.interval or options.decimal or 1,
					default = options.default or 10,
					value = options.default or 10, 

					ignore = options.ignore or false, 
					dragging = false,
				} 

				-- Instances 
					local object = library:create("TextLabel", {
						FontFace = library.font,
						TextColor3 = rgb(158, 158, 158),
						BorderColor3 = rgb(0, 0, 0),
						Text = "",
						Size = dim2(1, -26, 0, 29),
						BorderSizePixel = 0,
						BackgroundTransparency = 1,
						TextXAlignment = Enum.TextXAlignment.Left,
						TextYAlignment = Enum.TextYAlignment.Top,
						TextSize = 12,
						BackgroundColor3 = rgb(255, 255, 255),
						Parent = self.elements,
						Name = "",
					})

					local bottom_components = library:create("Frame", {
						Position = dim2(0, -1, 0, cfg.name and 15 or 0),
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, -5, 0, 0),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(255, 255, 255),
						Parent = object,
						Name = "",
					})

					local list = library:create("UIListLayout", {
						Padding = UDim.new(0, 10),
						SortOrder = Enum.SortOrder.LayoutOrder,
						Parent = bottom_components,
						Name = "",
					})

					local slider = library:create("Frame", {
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, 2, 1, 8),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(0, 0, 0),
						Parent = bottom_components,
						Name = "",
					})

					local slider = library:create("TextButton", {
						Position = dim2(0, 1, 0, 1),
						Text = "";
						AutoButtonColor = false;
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, -2, 1, -2),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(32, 32, 32),
						Parent = slider,
						Name = "",
					})

					local fill = library:create("Frame", {
						Size = dim2(0.5, 0, 1, 0),
						BorderColor3 = rgb(0, 0, 0),
						ZIndex = 5,
						BorderSizePixel = 0,
						BackgroundColor3 = themes.preset.accent,
						Parent = slider,
						Name = "",
					}) library:apply_theme(fill, "accent", "BackgroundColor3")

					local toggle_gradient = library:create("UIGradient", {
						Enabled = true,
						Rotation = 90,
						Color = ColorSequence.new{
							ColorSequenceKeypoint.new(0, rgb(255, 255, 255)),
							ColorSequenceKeypoint.new(1, rgb(126, 126, 126))
						},
						Parent = fill,
						Name = "",
					})

					local left_holder; 
					local text;
					if cfg.name then 
						left_holder = library:create("Frame", {
							BackgroundTransparency = 1,
							Position = dim2(0, 0, 0, 1),
							BorderColor3 = rgb(0, 0, 0),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = object,
							Name = "",
						})

						library:create("UIListLayout", {
							Padding = UDim.new(0, 7),
							SortOrder = Enum.SortOrder.LayoutOrder,
							Parent = left_holder,
							Name = "",
						})

						text = library:create("TextLabel", {
							FontFace = library.font,
							TextColor3 = rgb(221, 221, 221),
							BorderColor3 = rgb(0, 0, 0),
							Text = cfg.name,
							Size = dim2(1, -27, 0, 5),
							Position = dim2(0, 4, 0, -2),
							BorderSizePixel = 0,
							BackgroundTransparency = 1,
							TextXAlignment = Enum.TextXAlignment.Left,
							RichText = true,
							AutomaticSize = Enum.AutomaticSize.X,
							TextSize = 12,
							BackgroundColor3 = rgb(74, 74, 74),
							Parent = left_holder,
							Name = "",
						})
					end 

					local right_components = library:create("Frame", {
						Position = dim2(1, -5, 0, cfg.name and -2 or -17),
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(0, 0, 0, 12),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(255, 255, 255),
						Parent = object,
						Name = "",
					})

					local list = library:create("UIListLayout", {
						VerticalAlignment = Enum.VerticalAlignment.Center,
						FillDirection = Enum.FillDirection.Horizontal,
						HorizontalAlignment = Enum.HorizontalAlignment.Right,
						Padding = UDim.new(0, 4),
						SortOrder = Enum.SortOrder.LayoutOrder,
						Parent = right_components,
						Name = "",
					})

					local text = library:create("TextLabel", {
						FontFace = library.font,
						TextColor3 = rgb(221, 221, 221),
						BorderColor3 = rgb(0, 0, 0),
						Text = "4",
						Size = dim2(1, -30, 0, 5),
						Position = dim2(0, 4, 0, -2),
						BorderSizePixel = 0,
						BackgroundTransparency = 1,
						TextXAlignment = Enum.TextXAlignment.Left,
						RichText = true,
						AutomaticSize = Enum.AutomaticSize.X,
						TextSize = 12,
						BackgroundColor3 = rgb(74, 74, 74),
						Parent = right_components,
						Name = "",
					})                    
				-- 

				-- Functions 
					function cfg.set(value) 
						cfg.value = clamp(library:round(value, cfg.intervals), cfg.min, cfg.max)

						fill.Size = dim2((cfg.value - cfg.min) / (cfg.max - cfg.min), 0, 1, 0)
						text.Text = tostring(cfg.value) .. cfg.suffix

						flags[cfg.flag] = cfg.value

						cfg.callback(flags[cfg.flag])
					end 

					cfg.set(cfg.default)

					config_flags[cfg.flag] = cfg.set
				-- 

				-- Connections
					slider.MouseButton1Down:Connect(function()
						cfg.dragging = true 
					end)

					library:connection(uis.InputChanged, function(input)
						if cfg.dragging and input.UserInputType == Enum.UserInputType.MouseMovement then 
							local size_x = (input.Position.X - slider.AbsolutePosition.X) / slider.AbsoluteSize.X
							local value = ((cfg.max - cfg.min) * size_x) + cfg.min

							cfg.set(value)
						end
					end)

					library:connection(uis.InputEnded, function(input)
						if input.UserInputType == Enum.UserInputType.MouseButton1 then
							cfg.dragging = false 
						end 
					end)
				-- 

				return setmetatable(cfg, library)
			end 

			function library:keybind(options)
				local cfg = {
					flag = options.flag or "SET ME A FLAG NOWWW!!!!",
					callback = options.callback or function() end,
					open = false,
					binding = nil, 
					name = options.name or nil, 
					ignore_key = options.ignore or false, 

					key = options.key or nil, 
					mode = options.mode or "toggle",
					active = options.default or false, 

					hold_instances = {},
				}
				
				flags[cfg.flag] = {} 

				-- Instances
					-- Button 
						local gear_icon = library:create("ImageButton", {
							ImageColor3 = rgb(74, 74, 74),
							Image = "rbxassetid://81593156472608",
							BackgroundTransparency = 1,
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(0, 9, 0, 9),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = self.right_components,
							Name = "",
						})                    
					-- 

					-- The keybind text holder + input
						local keybind_outline = library:create("TextButton", {
							Visible = false,
							Text = "", 
							AutoButtonColor = false,
							Position = dim_offset(gear_icon.AbsolutePosition.X - 5, gear_icon.AbsolutePosition.Y - 5),
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(0, 0, 0, 19),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(0, 0, 0),
							Parent = library.gui,
							AutomaticSize = Enum.AutomaticSize.X,
							Name = "",
							ZIndex = 100,
						})

						library:create("UICorner", {
							CornerRadius = UDim.new(0, 4),
							Parent = keybind_outline,
							Name = "",
						})

						local keybind_inline = library:create("Frame", {
							Size = dim2(1, -2, 1, -2),
							ClipsDescendants = true,
							BorderColor3 = rgb(0, 0, 0),
							Position = dim2(0, 1, 0, 1),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(32, 32, 32),
							Parent = keybind_outline,
							Name = "",
						})

						library:create("UICorner", {
							CornerRadius = UDim.new(0, 4),
							Parent = keybind_inline,
							Name = "",
						})

						local keybind_background = library:create("Frame", {
							Size = dim2(1, -2, 1, -2),
							ClipsDescendants = true,
							BorderColor3 = rgb(0, 0, 0),
							Position = dim2(0, 1, 0, 1),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(8, 8, 8),
							Parent = keybind_inline,
							Name = "",
						})

						library:create("UIPadding", {
							PaddingTop = UDim.new(0, 18),
							PaddingBottom = UDim.new(0, 1),
							PaddingRight = UDim.new(0, -30),
							PaddingLeft = UDim.new(0, 1),
							Parent = keybind_background,
							Name = "",
						})

						local ImageButton = library:create("ImageButton", {
							BorderColor3 = rgb(0, 0, 0),
							AutoButtonColor = false,
							AnchorPoint = Vector2.new(0, 0.5),
							Image = "rbxassetid://81593156472608",
							BackgroundTransparency = 1,
							Position = dim2(0, 2, 0, -6),
							Size = dim2(0, 9, 0, 9),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = keybind_background,
							Name = "",
						})

						local ui_title = library:create("TextLabel", {
							FontFace = library.font,
							TextColor3 = rgb(221, 221, 221),
							BorderColor3 = rgb(0, 0, 0),
							Text = "no keybind",
							RichText = true,
							Size = dim2(0, 0, 1, 0),
							Position = dim2(0, 0, 0, -1),
							BackgroundTransparency = 1,
							TextXAlignment = Enum.TextXAlignment.Left,
							BorderSizePixel = 0,
							ZIndex = 2,
							AutomaticSize = Enum.AutomaticSize.X,
							TextSize = 12,
							BackgroundColor3 = rgb(74, 74, 74),
							Parent = keybind_outline,
							Name = "",
						})
						
						library:create("UIPadding", {
							PaddingTop = UDim.new(0, 0),
							PaddingBottom = UDim.new(0, 0),
							PaddingRight = UDim.new(0, 5),
							PaddingLeft = UDim.new(0, 5),
							Parent = ui_title,
							Name = "",
						})
					-- 

					-- Keybind list element
						local OPTION = library:create("TextLabel", {
							FontFace = library.font;
							TextColor3 = rgb(150, 150, 150);
							BorderColor3 = rgb(0, 0, 0);
							Text = "Speed Hack:[X]";
							Visible = false;
							Parent = library.keybind_list_parent;
							BackgroundTransparency = 1;
							Position = dim2(0, 3, 0, 5);
							BorderSizePixel = 0;
							AutomaticSize = Enum.AutomaticSize.XY;
							TextSize = 12;
							BackgroundColor3 = rgb(255, 255, 255)
						});
					-- 	
				-- 

				-- Functions 
					function cfg.set_mode(mode) 
						cfg.mode = mode 

						if mode == "always" then
							cfg.set(true)
						elseif mode == "hold" then
							cfg.set(false)
						end

						flags[cfg.flag]["mode"] = mode
					end 

					function cfg.set(input)
						if type(input) == "boolean" then 
							local __cached = input 

							if cfg.mode == "always" then 
								__cached = true 
							end 

							cfg.active = __cached 
							cfg.callback(__cached)
						elseif tostring(input):find("Enum") then 
							input = input.Name == "Escape" and "No key..." or input

							cfg.key = input or "No key..."	

							cfg.callback(cfg.active or false)
						elseif find({"toggle", "hold", "always"}, input) then 
							cfg.set_mode(input)

							if input == "always" then 
								cfg.active = true 
							end 

							cfg.callback(cfg.active or false)
						elseif type(input) == "table" then 
							input.key = type(input.key) == "string" and input.key ~= "No key..." and library:convert_enum(input.key) or input.key

							input.key = input.key == Enum.KeyCode.Escape and "No key..." or input.key
							cfg.key = input.key or "No key..."

							cfg.mode = input.mode or "toggle"

							if input.active then
								cfg.active = input.active
							end
						end 

						flags[cfg.flag] = {
							mode = cfg.mode,
							key = cfg.key, 
							active = cfg.active
						}

						local text = keys[cfg.key] or tostring(cfg.key):gsub("Enum.", ""):gsub("UserInputType.", "")

						ui_title.Text = "    Current keybind: " .. string.lower(text:gsub("KeyCode.", ""))

						if cfg.name then 
							OPTION.Visible = cfg.active
							OPTION.Text = cfg.name .. "[" .. string.upper(text:gsub("KeyCode.", "")) .. "]"
						end 
					end

					function cfg.set_visible(bool) 
						keybind_outline.Visible = bool
						keybind_outline.Position = dim_offset(gear_icon.AbsolutePosition.X - 5, gear_icon.AbsolutePosition.Y - 5 + gear_icon.AbsoluteSize.Y + 50)

						library:tween(library.shade, {
							BackgroundTransparency = bool and 0.75 or 1, 
						})	

						library.current = cfg
					end 

					config_flags[cfg.flag] = cfg.set
					cfg.set({mode = cfg.mode, active = cfg.active, key = cfg.key})
				-- 

				-- Connections 
					gear_icon.MouseButton1Down:Connect(function()
						cfg.set_visible(true)
					end)

					ImageButton.MouseButton1Down:Connect(function()
						cfg.set_visible(false)
					end)

					keybind_outline.MouseButton1Click:Connect(function()
						task.wait()
						ui_title.Text = "    Selecting..."	

						cfg.binding = library:connection(uis.InputBegan, function(keycode, game_event)  
							local selected_key = keycode.UserInputType == Enum.UserInputType.Keyboard and keycode.KeyCode or keycode.UserInputType
							cfg.set(selected_key)

							cfg.binding:Disconnect() 
							cfg.binding = nil
						end)
					end)

					gear_icon:GetPropertyChangedSignal("AbsolutePosition"):Connect(function()
						keybind_outline.Visible = false

						library:tween(library.shade, {
							BackgroundTransparency = 1, 
						})	
						
					end)

					library:connection(uis.InputBegan, function(input, game_event) 
						if game_event then 
							return 
						end 

						local selected_key = input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode or input.UserInputType

						if selected_key == cfg.key then 
							if cfg.mode == "toggle" then 
								cfg.active = not cfg.active
								cfg.set(cfg.active)
							elseif cfg.mode == "hold" then 
								cfg.set(true)
							end
						end
					end)

					library:connection(uis.InputEnded, function(input, game_event) 
						if game_event then 
							return 
						end 

						local selected_key = input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode or input.UserInputType

						if selected_key == cfg.key then
							if cfg.mode == "hold" then 
								cfg.set(false)
							end
						end
					end)
				--      

				return setmetatable(cfg, library)
			end

			function library:dropdown(options) 
				local cfg = {
					name = options.name or nil,
					flag = options.flag or options.name or "nigga",

					items = options.items or {"1", "2", "3"},
					callback = options.callback or function() end,
					multi = options.multi or false, 
					scrolling = options.scrolling or false, 

					-- Ignore these 
					open = false, 
					option_instances = {}, 
					multi_items = {}, 
					ignore = options.ignore or false, 
				}   

				cfg.default = options.default or (cfg.multi and {cfg.items[1]}) or cfg.items[1]

				flags[cfg.flag] = {} 

				-- Instances
					-- Element
						local object = library:create("TextLabel", {
							TextColor3 = rgb(158, 158, 158),
