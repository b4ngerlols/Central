							Size = dim2(1, -5, 1, 0),
							Position = dim2(0, 5, 0, -2),
							BorderSizePixel = 0,
							TextWrapped = true,
							BackgroundTransparency = 1,
							TextXAlignment = Enum.TextXAlignment.Left,
