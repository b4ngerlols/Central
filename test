	-- Library element functions
		function library:window(properties)
			local cfg = {
				name = properties.name or properties.Name or "rapando",
				size = properties.size or properties.Size or dim2(0, 600, 0, 430),
				selected_tab; -- This is what we will be using to manage the switching pages across index mts
			}

			library.gui = library:create("ScreenGui", {
				Parent = coregui,
				Name = "ITS MEE!!!",
				Enabled = true,
				ZIndexBehavior = Enum.ZIndexBehavior.Sibling,
				IgnoreGuiInset = true,
			})

			library.misc = library:create("ScreenGui", {
				Parent = coregui,
				Name = "",
				Enabled = true,
				ZIndexBehavior = Enum.ZIndexBehavior.Sibling,
				IgnoreGuiInset = true,
			})

			-- Window
				local window_outline = library:create("Frame", {
					Name = "outline",
					Position = dim2(0.5, 0 - (cfg.size.X.Offset / 2), 0.5, 0 - (cfg.size.X.Offset / 2)),
					BorderColor3 = rgb(0, 0, 0),
					Size = cfg.size,
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(0, 0, 0),
					Parent = library.gui,
				}); window_outline.Position = dim_offset(window_outline.AbsolutePosition.X, window_outline.AbsolutePosition.Y + 200)

				library:create("UICorner", {
					Name = "",
					CornerRadius = UDim.new(0, 4),
					Parent = window_outline,
				})

				library:draggify(window_outline)

				library.shade = library:create("Frame", {
					BackgroundTransparency = 1, -- 0.75
					BorderColor3 = rgb(0, 0, 0),
					Size = dim2(1, 0, 1, 0),
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(0, 0, 0),
					Parent = window_outline,
					Name = "",
					Visible = true,
					ZIndex = 99,
				})

				library:create("UICorner", {
					CornerRadius = UDim.new(0, 4),
					Parent = library.shade,
					Name = "",
				})

				local inline = library:create("Frame", {
					Size = dim2(1, -2, 1, -2),
					Name = "inline",
					ClipsDescendants = true,
					BorderColor3 = rgb(0, 0, 0),
					Position = dim2(0, 1, 0, 1),
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(32, 32, 32),
					Parent = window_outline,
				})

				local outline = library:create("Frame", {
					AnchorPoint = Vector2.new(0, 1),
					Name = "outline",
					Position = dim2(0, 1, 1, -1),
					BorderColor3 = rgb(0, 0, 0),
					Size = dim2(1, -2, 0, 20),
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(0, 0, 0),
					Parent = inline,
				})

				local element_holder = library:create("Frame", {
					Name = "element_holder",
					Position = dim2(0, 1, 0, 1),
					BorderColor3 = rgb(0, 0, 0),
					Size = dim2(1, -2, 1, -2),
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(8, 8, 8),
					Parent = outline,
				})

				local fill = library:create("Frame", {
					Size = dim2(1, 2, 0, 1),
					Name = "fill",
					Position = dim2(0, -1, 0, -1),
					BorderColor3 = rgb(0, 0, 0),
					ZIndex = 0,
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(0, 0, 0),
					Parent = element_holder,
				})

				library:create("UICorner", {
					Name = "",
					CornerRadius = UDim.new(0, 4),
					Parent = element_holder,
				})

				local build = library:create("TextLabel", {
					FontFace = library.font,
					TextColor3 = themes.preset.accent,
					BorderColor3 = rgb(0, 0, 0),
					Text = '<font color="#4A4A4A">build</font>: live',
