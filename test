				}) library:apply_theme(build, "accent", "BackgroundColor3")

				library:create("UICorner", {
					Name = "",
					CornerRadius = UDim.new(0, 4),
					Parent = outline,
				})

				local page_element_holder = library:create("Frame", {
					Name = "page_element_holder",
