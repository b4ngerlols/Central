							Parent = colorpicker_inline;
							Size = dim2(1, -2, 1, -2);
							ClipsDescendants = true;
							BorderColor3 = rgb(0, 0, 0);
							Position = dim2(0, 1, 0, 1);
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(8, 8, 8)
						});
						
						library:create("UICorner", {
							Parent = colorpicker_background;
							CornerRadius = dim(0, 4)
						});
						
						library:create("UIPadding", {
							PaddingTop = dim(0, 16);
							PaddingBottom = dim(0, 1);
							Parent = colorpicker_background;
							PaddingRight = dim(0, 1);
							PaddingLeft = dim(0, 1)
						});
						
						local saturation_outline = library:create("Frame", {
							Parent = colorpicker_background;
							BorderColor3 = rgb(0, 0, 0);
							Size = dim2(1, -12, 1, -12);
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(0, 0, 0)
						});
						
						local color_saturation = library:create("Frame", {
							Parent = saturation_outline;
							Position = dim2(0, 1, 0, 1);
							BorderColor3 = rgb(0, 0, 0);
							Size = dim2(1, -2, 1, -2);
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(255, 39, 39)
						});
						
						local sat = library:create("TextButton", {
							Parent = color_saturation;
							AutoButtonColor = false;
							Text = "";
							Size = dim2(1, 0, 1, 0);
							BorderColor3 = rgb(0, 0, 0);
							ZIndex = 2;
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(255, 255, 255)
						});
						
						library:create("UIGradient", {
							Rotation = 270;
							Transparency = numseq{numkey(0, 0), numkey(1, 1)};
							Parent = sat;
							Color = rgbseq{rgbkey(0, rgb(0, 0, 0)), rgbkey(1, rgb(0, 0, 0))}
						});
						
						local satval_picker = library:create("Frame", {
							Parent = color_saturation;
							Size = dim2(0, 3, 0, 3);
							Position = dim2(0, 1, 0.5, 1);
							BorderColor3 = rgb(0, 0, 0);
							ZIndex = 4;
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(0, 0, 0)
						});
						
						library:create("Frame", {
							Parent = satval_picker;
							Size = dim2(1, -2, 1, -2);
							Position = dim2(0, 1, 0, 1);
							BorderColor3 = rgb(0, 0, 0);
							ZIndex = 2;
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(255, 255, 255)
						});
						
						local val = library:create("TextButton", {
							Parent = color_saturation;
							AutoButtonColor = false;
							Text = "";
							BorderColor3 = rgb(0, 0, 0);
							Size = dim2(1, 0, 1, 0);
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(255, 255, 255)
						});
						
						library:create("UIGradient", {
							Parent = val;
							Transparency = numseq{numkey(0, 0), numkey(1, 1)}
						});
						
						local hue_slider = library:create("TextButton", {
							Parent = colorpicker_background;
							Position = dim2(1, -10, 0, 0);
							BorderColor3 = rgb(0, 0, 0);
							Size = dim2(0, 10, 1, -12);
							BorderSizePixel = 0;
							AutoButtonColor = false; 
							Text = "";
							BackgroundColor3 = rgb(0, 0, 0)
						});
						
						local hue_components = library:create("Frame", {
							Parent = hue_slider;
							Position = dim2(0, 1, 0, 1);
							BorderColor3 = rgb(0, 0, 0);
							Size = dim2(1, -2, 1, -2);
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(255, 255, 255)
						});
						
						library:create("UIGradient", {
							Rotation = 270;
							Parent = hue_components;
							Color = rgbseq{rgbkey(0, rgb(255, 0, 0)), rgbkey(0.17, rgb(255, 255, 0)), rgbkey(0.33, rgb(0, 255, 0)), rgbkey(0.5, rgb(0, 255, 255)), rgbkey(0.67, rgb(0, 0, 255)), rgbkey(0.83, rgb(255, 0, 255)), rgbkey(1, rgb(255, 0, 0))}
						});
						
						local hue_picker = library:create("Frame", {
							Parent = hue_components;
							Size = dim2(1, 2, 0, 3);
							Position = dim2(0, -1, 0, -1);
							BorderColor3 = rgb(0, 0, 0);
							ZIndex = 4;
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(0, 0, 0)
						});
						
						library:create("Frame", {
							Parent = hue_picker;
							Size = dim2(1, -2, 1, -2);
							Position = dim2(0, 1, 0, 1);
							BorderColor3 = rgb(0, 0, 0);
							ZIndex = 2;
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(255, 255, 255)
						});
						
						local alpha_slider = library:create("Frame", {
							Parent = colorpicker_background;
							Position = dim2(0, 0, 1, -10);
							BorderColor3 = rgb(0, 0, 0);
							Size = dim2(1, -12, 0, 10);
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(0, 0, 0)
						});
						
						local alpha_components = library:create("TextButton", {
							Parent = alpha_slider;
							Text = "";
							AutoButtonColor = false;
							Position = dim2(0, 1, 0, 1);
							BorderColor3 = rgb(0, 0, 0);
							Size = dim2(1, -2, 1, -2);
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(255, 255, 255)
						});
						
						library:create("UIGradient", {
							Color = rgbseq{rgbkey(0, rgb(0, 0, 0)), rgbkey(1, rgb(255, 255, 255))};
							Parent = alpha_components
						});
						
						local alpha_picker = library:create("Frame", {
							Parent = alpha_components;
							Size = dim2(0, 3, 1, 2);
							Position = dim2(0, -1, 0, -1);
							BorderColor3 = rgb(0, 0, 0);
							ZIndex = 4;
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(0, 0, 0)
						});
						
						library:create("Frame", {
							Parent = alpha_picker;
							Size = dim2(1, -2, 1, -2);
							Position = dim2(0, 1, 0, 1);
							BorderColor3 = rgb(0, 0, 0);
							ZIndex = 2;
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(255, 255, 255)
						});
						
						local gear_2 = library:create("ImageButton", {
							Parent = colorpicker_background;
							Image = "rbxassetid://81593156472608";
							BackgroundTransparency = 1;
							Position = dim2(0, 2, 0, -14);
							BorderColor3 = rgb(0, 0, 0);
							Size = dim2(0, 9, 0, 9);
							BorderSizePixel = 0;
							BackgroundColor3 = rgb(255, 255, 255)
						});
					-- 
				-- 
			
				-- Functions 
					local dragging_sat = false 
					local dragging_hue = false 
					local dragging_alpha = false 

					local h, s, v = cfg.color:ToHSV() 
					local a = cfg.alpha 

					flags[cfg.flag] = {} 

					function cfg.set_visible(bool) 
						colorpicker_outline.Visible = bool
						library.current = cfg
					end

					function cfg.set(color, alpha)
						if color then
							h, s, v = color:ToHSV()
						end
						
						if alpha then 
							a = alpha
						end 
						
						local Color = Color3.fromHSV(h, s, v)
						
						hue_picker.Position = dim2(0, -1, 1 - h, -1)
						alpha_picker.Position = dim2(1 - a, -1, 0, -1)
						satval_picker.Position = dim2(s, -1, 1 - v, -1)

						color_saturation.BackgroundColor3 = Color3.fromHSV(h, 1, 1)
						
						flags[cfg.flag] = {
							Color = Color;
							Transparency = a 
						}
						
						cfg.callback(Color, a)
					end

					function cfg.update_color() 
						local mouse = uis:GetMouseLocation() 
						local offset = vec2(mouse.X, mouse.Y - gui_offset) 

						if dragging_sat then	
							s = math.clamp((offset - sat.AbsolutePosition).X / sat.AbsoluteSize.X, 0, 1)
							v = 1 - math.clamp((offset - val.AbsolutePosition).Y / val.AbsoluteSize.Y, 0, 1)
						elseif dragging_hue then
							h = 1 - math.clamp((offset - hue_slider.AbsolutePosition).Y / hue_slider.AbsoluteSize.Y, 0, 1)
						elseif dragging_alpha then
							a = 1 - math.clamp((offset - alpha_components.AbsolutePosition).X / alpha_components.AbsoluteSize.X, 0, 1)
						end

						cfg.set(nil, nil)
					end

					cfg.set(cfg.color, cfg.alpha)

					config_flags[cfg.flag] = cfg.set
				-- 

				-- Connections 
					gear_icon.MouseButton1Down:Connect(function()
						library:tween(library.shade, {
							BackgroundTransparency = 0.75, 
						})
						
						colorpicker_outline.Visible = true
						colorpicker_outline.Position = dim_offset(gear_icon.AbsolutePosition.X - 5, gear_icon.AbsolutePosition.Y - 5 + gear_icon.AbsoluteSize.Y + 50)
					end)
			
					gear_2.MouseButton1Down:Connect(function()
						library:tween(library.shade, {
							BackgroundTransparency = 1, 
						})	
			
						colorpicker_outline.Visible = false	
					end)
			
					gear_icon:GetPropertyChangedSignal("AbsolutePosition"):Connect(function()
						colorpicker_outline.Visible = false
			
						library:tween(library.shade, {
							BackgroundTransparency = 1, 
						})	
					end)	

					uis.InputChanged:Connect(function(input)
                        if (dragging_sat or dragging_hue or dragging_alpha) and input.UserInputType == Enum.UserInputType.MouseMovement then
                            cfg.update_color() 
                        end
                    end)

                    library:connection(uis.InputEnded, function(input)
                        if input.UserInputType == Enum.UserInputType.MouseButton1 then
                            dragging_sat = false
                            dragging_hue = false
                            dragging_alpha = false  
                        end
                    end)

                    alpha_components.MouseButton1Down:Connect(function()
                        dragging_alpha = true 
                    end)
                    
                    hue_slider.MouseButton1Down:Connect(function()
                        dragging_hue = true 
                    end)
                    
                    sat.MouseButton1Down:Connect(function()
                        dragging_sat = true  
                    end)
				--      
						
				return setmetatable(cfg, library)
			end 

			function library:button(options)
				local cfg = {
                    name = options.name or "button",
                    callback = options.callback or function() end,
                }

				-- Instances
					local object = library:create("TextLabel", {
						FontFace = library.font;
						TextColor3 = rgb(158, 158, 158);
						BorderColor3 = rgb(0, 0, 0);
						Text = "";
						Parent = self.elements;
						Size = dim2(1, -26, 0, 0);
						BorderSizePixel = 0;
						BackgroundTransparency = 1;
						TextXAlignment = Enum.TextXAlignment.Left;
						AutomaticSize = Enum.AutomaticSize.Y;
						TextYAlignment = Enum.TextYAlignment.Top;
						TextSize = 11;
						BackgroundColor3 = rgb(255, 255, 255)
					});
					
					local bottom_components = library:create("Frame", {
						Parent = object;
						Position = dim2(0, -1, 0, 0);
						BorderColor3 = rgb(0, 0, 0);
						Size = dim2(1, -5, 0, 0);
						BorderSizePixel = 0;
						BackgroundColor3 = rgb(255, 255, 255)
					});
					
					library:create("UIListLayout", {
						Parent = bottom_components;
						Padding = dim(0, 10);
						SortOrder = Enum.SortOrder.LayoutOrder
					});
					
					local button_outline = library:create("TextButton", {
						Parent = bottom_components;
						AutoButtonColor = false;
						Text = "";
						BorderColor3 = rgb(0, 0, 0);
						Size = dim2(1, 2, 1, 20);
						BorderSizePixel = 0;
						BackgroundColor3 = rgb(0, 0, 0)
					});
					
					local button_inline = library:create("Frame", {
						Parent = button_outline;
						Position = dim2(0, 1, 0, 1);
						BorderColor3 = rgb(0, 0, 0);
						Size = dim2(1, -2, 1, -2);
						BorderSizePixel = 0;
						BackgroundColor3 = rgb(255, 255, 255)
					});
					
					library:create("UIGradient", {
						Rotation = 90;
						Parent = button_inline;
						Color = rgbseq{rgbkey(0, rgb(23, 23, 23)), rgbkey(1, rgb(16, 16, 16))}
					});
					
					local text = library:create("TextLabel", {
						TextWrapped = true;
						TextColor3 = rgb(74, 74, 74);
						BorderColor3 = rgb(0, 0, 0);
						Text = cfg.name;
						Parent = button_inline;
						Size = dim2(1, 0, 1, 0);
						Position = dim_offset(0, -2);
						BorderSizePixel = 0;
						BackgroundTransparency = 1;
						RichText = true;
						FontFace = library.font;
						AutomaticSize = Enum.AutomaticSize.X;
						TextSize = 12;
						BackgroundColor3 = rgb(74, 74, 74)
					});
					
					library:create("UICorner", {
						Parent = button_inline;
						CornerRadius = dim(0, 3)
					});
					
					library:create("UICorner", {
						Parent = button_outline;
						CornerRadius = dim(0, 3)
					});
				-- 

				-- Connections 
					button_outline.MouseButton1Click:Connect(function()
						cfg.callback()

						task.spawn(function()
							library:tween(text, {TextColor3 = themes.preset.accent}, 0.15)
							task.wait(0.15)
							library:tween(text, {TextColor3 = rgb(74, 74, 74)}, 0.15)
						end)
					end)
				-- 
				
				return setmetatable(cfg, library)
			end 
			
			function library:list(options) 
				local cfg = {
					name = options.name or nil,
					flag = options.flag or options.name or "nigga",

					items = options.items or {"1", "2", "3"},
					callback = options.callback or function() end,
					multi = options.multi or false, 
					scrolling = options.scrolling or false, 

					-- Ignore these 
					open = false, 
					option_instances = {}, 
					multi_items = {}, 
					ignore = options.ignore or false, 
				}   

				cfg.default = options.default or (cfg.multi and {cfg.items[1]}) or cfg.items[1]

				flags[cfg.flag] = {} 

				-- Instances
					-- Element
						local object = library:create("TextLabel", {
							TextColor3 = rgb(158, 158, 158),
							BorderColor3 = rgb(0, 0, 0),
							Text = "",
							Size = dim2(1, -26, 0, 0),
							BorderSizePixel = 0,
							BackgroundTransparency = 1,
							TextXAlignment = Enum.TextXAlignment.Left,
							TextYAlignment = Enum.TextYAlignment.Top,
							AutomaticSize = Enum.AutomaticSize.Y;
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = self.elements,
							Name = "",
						})

						local bottom_components = library:create("Frame", {
							Position = dim2(0, -1, 0, 0),
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(1, -3, 0, 0),
							BackgroundTransparency = 1,
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = object,
							AutomaticSize = Enum.AutomaticSize.Y;
							Name = "",
						}) 
					-- 

					-- Textbox
						local holder = library:create("Frame", {
							Size = dim2(1, 0, 0, 20),
							BorderColor3 = rgb(0, 0, 0),
							BorderSizePixel = 0,
							Visible = true, 
							BackgroundColor3 = rgb(0, 0, 0),
							Parent = bottom_components,
							Name = "",
						})

						local holder_inline = library:create("Frame", {
							Size = dim2(1, -2, 1, -2),
							Position = dim2(0, 1, 0, 1),
							BorderColor3 = rgb(0, 0, 0),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = holder,
							Name = "",
						})

						local textbox = library:create("TextBox", {
                            FontFace = library.font;
                            TextColor3 = rgb(74, 74, 74);
                            BorderColor3 = rgb(0, 0, 0);
                            Text = "";
                            Parent = holder_inline;
                            BackgroundTransparency = 1;
                            ClearTextOnFocus = false;
                            PlaceholderColor3 = rgb(255, 255, 255);
                            Size = dim2(1, 0, 1, 0);
                            Position = dim2(1, 0, 1, -2);
                            BorderSizePixel = 0;
                            TextSize = 12;
                            TextXAlignment = Enum.TextXAlignment.Center;
                        });

						library:create("UIGradient", {
							Rotation = 90,
							Color = ColorSequence.new{
								ColorSequenceKeypoint.new(0, rgb(23, 23, 23)),
								ColorSequenceKeypoint.new(1, rgb(16, 16, 16))
							},
							Parent = holder_inline,
							Name = "",
						})

						library:create("UIListLayout", {
							Padding = UDim.new(0, 9),
							SortOrder = Enum.SortOrder.LayoutOrder,
							Parent = holder_inline,
							Name = "",
						})

						library:create("UIPadding", {
							PaddingBottom = UDim.new(0, 9),
							PaddingTop = UDim.new(0, 6),
							PaddingLeft = UDim.new(0, 4),
							Parent = holder_inline,
							Name = "",
						})
					-- 

					-- Holder
						local size = 100 

						local text_holder = library:create("Frame", {
							Size = dim2(1, 0, 0, size - 25),
							Position = dim_offset(0, 25),
							BorderColor3 = rgb(0, 0, 0),
							BorderSizePixel = 0,
							Visible = true, 
							BackgroundColor3 = rgb(0, 0, 0),
							Parent = bottom_components,
							Name = "",
							ZIndex = 0;
						})

						local inline = library:create("Frame", {
							Size = dim2(1, -2, 1, -2),
							Position = dim_offset(1, 1),
							BorderColor3 = rgb(0, 0, 0),
							BorderSizePixel = 0,
							Visible = true, 
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = text_holder,
							Name = "",
							ZIndex = 0;
						})

						local text_holder_inline = library:create("ScrollingFrame", {
							Size = dim2(1, -2, 1, -2),
							Position = dim2(0, 1, 0, 1),
							BorderColor3 = rgb(0, 0, 0),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = text_holder,
							Name = "",
							ScrollBarImageColor3 = rgb(31, 31, 31),
							Active = true,
							AutomaticCanvasSize = Enum.AutomaticSize.Y,
							ScrollBarThickness = 3,
							BackgroundTransparency = 1,
							CanvasSize = dim2(0, 0, 0, 0),
						})
						
						library:create("UIGradient", {
							Rotation = 90,
							Color = ColorSequence.new{
								ColorSequenceKeypoint.new(0, rgb(23, 23, 23)),
								ColorSequenceKeypoint.new(1, rgb(16, 16, 16))
							},
							Parent = inline,
							Name = "",
						})

						library:create("UIListLayout", {
							Padding = UDim.new(0, 3),
							SortOrder = Enum.SortOrder.LayoutOrder,
							Parent = text_holder_inline,
							Name = "",
						})

						library:create("UIPadding", {
							PaddingBottom = UDim.new(0, 9),
							PaddingTop = UDim.new(0, 6),
							PaddingLeft = UDim.new(0, 4),
							Parent = text_holder_inline,
							Name = "",
						})
					-- 
				-- 

				-- Functions
					function cfg.render_option(text) 
						-- Button Instances
							local tab_button = library:create("Frame", {
								BorderColor3 = rgb(0, 0, 0),
								Size = dim2(1, 0, 0, 18),
								BorderSizePixel = 0,
								BackgroundColor3 = rgb(255, 255, 255),
								BackgroundTransparency = 1, --0.9
								Parent = text_holder_inline,
								Name = "",
							})

							local accent = library:create("Frame", {
								BorderColor3 = rgb(0, 0, 0),
								Size = dim2(0, 1, 1, 0),
								BorderSizePixel = 0,
								BackgroundColor3 = themes.preset.accent,
								Parent = tab_button,
								BackgroundTransparency = 1,
								Name = "",
							}) library:apply_theme(accent, "accent", "BackgroundColor3")

							local UIGradient = library:create("UIGradient", {
								Rotation = 180,
								Color = ColorSequence.new{
									ColorSequenceKeypoint.new(0, themes.preset.accent),
									ColorSequenceKeypoint.new(1, themes.preset.accent)
								},
								Transparency = numseq{numkey(0, 1), numkey(1, 0)},
								Enabled = true,
								Parent = tab_button,
								Name = "",
							}) library:apply_theme(UIGradient, "accent", "Color")

							local ui_title = library:create("TextButton", {
								FontFace = library.font,
								TextColor3 = rgb(65, 65, 65),
								BorderColor3 = rgb(0, 0, 0),
								Text = text,
								Size = dim2(1, -7, 1, 0),
								BackgroundTransparency = 1,
								TextXAlignment = Enum.TextXAlignment.Left,
								Position = dim2(0, 7, 0, -2),
								BorderSizePixel = 0,
								TextSize = 12,
								BackgroundColor3 = rgb(255, 255, 255),
								Parent = tab_button,
								Name = "",
							}) library:apply_theme(ui_title, "accent", "TextColor3")
						--
							
						return {tab_button, accent, UIGradient, ui_title} 
					end 
				
					function cfg.refresh_options(options)
						for _, v in next, cfg.option_instances do 
							v[1]:Destroy() 
						end
				
						for _, option in next, options do 
							local button = cfg.render_option(option) 
				
							insert(cfg.option_instances, button)
							
							button[4].MouseButton1Click:Connect(function()
								if cfg.current_instance and cfg.current_instance ~= button then 
									library:tween(cfg.current_instance[4], {TextColor3 = rgb(74, 74, 74)}, 0.25)
									library:tween(cfg.current_instance[1], {BackgroundTransparency = 1}, 0.25)
									library:tween(cfg.current_instance[2], {BackgroundTransparency = 1}, 0.25)
								end
								
								cfg.current_instance = button
								library:tween(button[4], {TextColor3 = rgb(255, 255, 255)}, 0.25)
								library:tween(button[1], {BackgroundTransparency = 0.9}, 0.25)
								library:tween(button[2], {BackgroundTransparency = 0}, 0.25)

								flags[cfg.flag] = button[4].text
								
								cfg.callback(button.text)
							end)
						end 
					end
					
					function cfg.filter_options(text)
						for _, v in next, cfg.option_instances do 
							if string.find(v[4].Text, string.lower(text)) then 
								v[1].Parent = text_holder_inline 
							else
								v[1].Parent = library.cache
							end
						end
					end
					
					function cfg.set(value)
						for _, buttons in next, cfg.option_instances do 
							if buttons[4].Text == value then 
								buttons.TextColor3 = themes.preset.accent 
							else 
								buttons.TextColor3 = rgb(180, 180, 180) 
							end 
						end 
						
						flags[cfg.flag] = value
						cfg.callback(value)
					end 
					
					cfg.refresh_options(cfg.items) 
				-- 

				library.config_flags[cfg.flag] = cfg.set

				-- Connections
					textbox:GetPropertyChangedSignal("Text"):Connect(function()
						cfg.filter_options(textbox.Text)
					end)
				--

				return setmetatable(cfg, library)
			end

			function library:textbox(options)
				local cfg = {
                    name = options.name or "TextBox",
                    placeholder = options.placeholder or options.placeholdertext or options.holder or options.holdertext or "type here...",
                    default = options.default,
                    flag = options.flag or "SET ME NIGGA",
                    callback = options.callback or function() end,
                    visible = options.visible or true,
                }
				
				-- Instances
					-- Element
						local object = library:create("TextLabel", {
							TextColor3 = rgb(158, 158, 158),
							BorderColor3 = rgb(0, 0, 0),
							Text = "",
							Size = dim2(1, -26, 0, 0),
							BorderSizePixel = 0,
							BackgroundTransparency = 1,
							TextXAlignment = Enum.TextXAlignment.Left,
							TextYAlignment = Enum.TextYAlignment.Top,
							AutomaticSize = Enum.AutomaticSize.Y;
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = self.elements,
							Name = "",
						})

						local bottom_components = library:create("Frame", {
							Position = dim2(0, -1, 0, 0),
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(1, -3, 0, 0),
							BackgroundTransparency = 1,
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = object,
							AutomaticSize = Enum.AutomaticSize.Y;
							Name = "",
						}) 
					-- 

					-- Textbox
						local holder = library:create("Frame", {
							Size = dim2(1, 0, 0, 20),
							BorderColor3 = rgb(0, 0, 0),
							BorderSizePixel = 0,
							Visible = true, 
							BackgroundColor3 = rgb(0, 0, 0),
							Parent = bottom_components,
							Name = "",
						})

						local holder_inline = library:create("Frame", {
							Size = dim2(1, -2, 1, -2),
							Position = dim2(0, 1, 0, 1),
							BorderColor3 = rgb(0, 0, 0),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = holder,
							Name = "",
						})

						local textbox = library:create("TextBox", {
							FontFace = library.font;
							TextColor3 = rgb(74, 74, 74);
							BorderColor3 = rgb(0, 0, 0);
							Text = "";
							Parent = holder_inline;
							BackgroundTransparency = 1;
							ClearTextOnFocus = false;
							PlaceholderColor3 = rgb(255, 255, 255);
							Size = dim2(1, 0, 1, 0);
							Position = dim2(1, 0, 1, -2);
							BorderSizePixel = 0;
							TextSize = 12;
							TextXAlignment = Enum.TextXAlignment.Center;
						});

						library:create("UIGradient", {
							Rotation = 90,
							Color = ColorSequence.new{
								ColorSequenceKeypoint.new(0, rgb(23, 23, 23)),
								ColorSequenceKeypoint.new(1, rgb(16, 16, 16))
							},
							Parent = holder_inline,
							Name = "",
						})

						library:create("UIListLayout", {
							Padding = UDim.new(0, 9),
							SortOrder = Enum.SortOrder.LayoutOrder,
							Parent = holder_inline,
							Name = "",
						})

						library:create("UIPadding", {
							PaddingBottom = UDim.new(0, 9),
							PaddingTop = UDim.new(0, 6),
							PaddingLeft = UDim.new(0, 4),
							Parent = holder_inline,
							Name = "",
						})
					-- 
				--
				
				-- Functions
					function cfg.set(text) 
						flags[cfg.flag] = text
						textbox.Text = text
						cfg.callback(text)
					end 

					if cfg.default then 
						cfg.set(cfg.default) 
					end 

					config_flags[cfg.flag] = cfg.set
				-- 

				-- Connections 
					textbox:GetPropertyChangedSignal("Text"):Connect(function()
						cfg.set(textbox.Text) 
					end)
				-- 
				
				return setmetatable(cfg, library)
			end 
		-- 
	-- 
-- 

local window = library:window({
	name = "rapando",
	size = dim2(0, 600, 0, 430)
})  

-- Rage Tab
local aimbot, exploits, antiaim = window:tab({name = "Rage", sections = {"Aimbot", "Exploits", "Anti-Aim"}})
local settings = window:tab({name = "Profile", sections = {"Settings"}})

-- Rage Tab
	local gear, automatic, rifle, shotgun, revolver = aimbot:multi_section({
		icons = {
			"104303181295923",
			"79566962223363",
			"76340056363612",
			"135518637762286",
			"76889014784999"
		},
		sizes = {
			dim_offset(9, 9),
			dim_offset(64, 27),
			dim_offset(64, 27),
			dim_offset(64, 27),
			dim_offset(64, 27)
		}
	})

	for _, page in {aimbot, exploits, antiaim} do 
		local gear, automatic, rifle, shotgun, revolver = page:multi_section({
			icons = {
				"104303181295923",
				"79566962223363",
				"76340056363612",
				"135518637762286",
				"76889014784999"
			},
			sizes = {
				dim_offset(9, 9),
				dim_offset(64, 27),
				dim_offset(64, 27),
				dim_offset(64, 27),
				dim_offset(64, 27)
			}
		})

		local tabs = {"gear", "automatic", "rifle", "shotgun", "revolver"}
		for _,tab in {gear, automatic, rifle, shotgun, revolver} do 
			local column = tab:column() 
			local general = column:section({name = tabs[_]})

			local column = tab:column()
			local accuracy = column:section({name = "Accuracy"})

			general:toggle({name = "Enabled"})
			general:toggle({name = "Silent aim"})
			general:toggle({name = "Automatic fire"})
			general:toggle({name = "Automatic knife"})
			general:toggle({name = "Dormant Aimbot"})
			general:slider({name = "Targets per tick", intervals = 1, min = 1, max = 10, default = 4})
			general:label({name = "Roll resolver"})
				:keybind({name = "Roll Resolver", callback = function(bool) print(bool) end})
			general:dropdown({name = "Hitboxes", items = {"Head", "Pelvis", "Torso", "Feet"}})
			general:dropdown({name = "Multipoints", items = {"Head", "Pelvis", "Torso", "Feet", "Pelvis", "Pelvis", "Pelvis"}})
			general:slider({name = "Headscale", intervals = 1, min = 1, max = 10, default = 0, suffix = "%", flag = "Headscale"})
			general:slider({name = "Pointscale", intervals = 1, min = 1, max = 100000, default = 0, suffix = "%"})
			local toggle = general:toggle({name = "Dynamic pointscale"})
				:colorpicker({name = "hello!", flag = "killniggas"})
			local list = general:list({name = "Hi!"})
			local button = general:button({name = "Click me!", callback = function() print("clicked!") end})
		end 
	end 
	
-- 

-- Profile
	local old_config;
	local column = settings:column() 
	local config_section = column:section({name = "Config"});
	local column = settings:column() 
	local config_other = column:section({name = "Other"});

	config_holder = config_section:list({name = "Select", items = {"none"}, defult = "none", multi = false, flag = "config_name_list"})
	config_section:textbox({name = "Config name", placeholder = "name...", flag = "config_name_text_box"})

	config_section:button({name = "Create", callback = function()
		if flags["config_name_text_box"] == "" then 
			return 
		end 

		writefile(library.directory .. "/configs/" .. flags["config_name_text_box"] .. ".cfg", library:get_config())

		library:update_cfg_list()
	end})

	config_section:button({name = "Delete", callback = function()
		delfile(library.directory .. "/configs/" .. flags["config_name_list"] .. ".cfg")
		library:update_cfg_list()
	end})

	config_section:button({name = "Load", callback = function()
		library:load_config(readfile(library.directory .. "/configs/" .. flags["config_name_list"] .. ".cfg"))
	end})

	config_section:button({name = "Refresh configs", callback = function()
		library:update_cfg_list()
	end}); library:update_cfg_list()

	config_other:label({name = "Accent"})	
				:colorpicker({callback = function(color) library:update_theme("accent", color) end})

	config_other:toggle({name = "Keybind List", callback = function(bool)
		library.misc.Enabled = bool
	end})

	config_section:label({name = "Menu Bind"}):keybind({callback = function(bool) 
		window.toggle_menu(bool)
	end})

	config_other:button({name = "Unload Config", callback = function()
		library:load_config(old_config)
	end})

	config_other:button({name = "Unload Menu", callback = function()
		library:unload_menu()
	end})
	
	old_config = library:get_config()
--

for i = 1, 30  do 
	task.wait(0.1)
	notifications:create_notification({name = "notif abc"})
end
