					BackgroundColor3 = rgb(0, 0, 0),
					Parent = inline,
				})

				local ui_title = library:create("TextLabel", {
					FontFace = library.font,
					TextColor3 = themes.preset.accent,
					BorderColor3 = rgb(0, 0, 0),
					Text = cfg.name,
					Name = "ui_title",
					RichText = true,
					Size = dim2(1, -4, 1, 0),
					Position = dim2(0, 4, 0, -2),
					BackgroundTransparency = 1,
					TextXAlignment = Enum.TextXAlignment.Left,
					BorderSizePixel = 0,
					ZIndex = 1000,
					TextSize = 12,
					BackgroundColor3 = rgb(74, 74, 74),
					Parent = outline,
				}) library:apply_theme(ui_title, "accent", "TextColor3")

				local tab_holder = library:create("Frame", {
					Name = "tab_holder",
					Position = dim2(0, 1, 0, 1),
					BorderColor3 = rgb(0, 0, 0),
					Size = dim2(1, -2, 1, -2),
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(8, 8, 8),
					Parent = outline,
				}); cfg.tab_holder = tab_holder;
				
				local accent = library:create("Frame", {
					Parent = tab_holder;
					Position = dim2(0, 150, 1, 3);
					BorderColor3 = rgb(0, 0, 0);
					Size = dim2(1, -150, 0, 1);
					BorderSizePixel = 0;
					BackgroundColor3 = themes.preset.accent
				}); library:apply_theme(accent, "accent", "BackgroundColor3")

				library:create("UIGradient", {
					Parent = accent;
					Transparency = numseq{numkey(0, 1), numkey(1, 0)}
				});			

				local fill = library:create("Frame", {
					Size = dim2(1, 2, 0, 1),
					Name = "fill",
					Position = dim2(0, -1, 1, 0),
					BorderColor3 = rgb(0, 0, 0),
					ZIndex = 0,
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(0, 0, 0),
					Parent = tab_holder,
				})

				local tab_button_holder = library:create("Frame", {
					Name = "tab_button_holder",
					BackgroundTransparency = 1,
					Position = dim2(0, 94, 0, 0),
					BorderColor3 = rgb(0, 0, 0),
					Size = dim2(1, -94, 1, 0),
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(255, 255, 255),
					Parent = tab_holder,
				}); cfg.button_holder = tab_button_holder; 
				
				local fade = library:create("Frame", {
					BorderColor3 = rgb(0, 0, 0),
					BackgroundTransparency = 1,
					Position = dim2(0, -2, 0, 0),
					Size = dim2(1, 0, 1, 0),
					ZIndex = 6,
					BorderSizePixel = 0,
					BackgroundColor3 = rgb(8, 8, 8),
					Parent = tab_holder,
					Name = "",
				})

				local UIPadding = library:create("UIPadding", {
					PaddingTop = UDim.new(0, 1),
					Parent = tab_button_holder,
				})

				local UIListLayout = library:create("UIListLayout", {
					Padding = UDim.new(0, 4),
					SortOrder = Enum.SortOrder.LayoutOrder,
					FillDirection = Enum.FillDirection.Horizontal,
					Parent = tab_button_holder,
				})

				library:create("UICorner", {
					Name = "",
					CornerRadius = UDim.new(0, 4),
					Parent = tab_holder,
				})

				library:create("UICorner", {
					Name = "",
					CornerRadius = UDim.new(0, 4),
					Parent = outline,
				})

				library:create("UICorner", {
					Name = "",
					CornerRadius = UDim.new(0, 4),
					Parent = inline,
				})

				local UICorner = library:create("UICorner", {
					CornerRadius = UDim.new(0, 4),
					Parent = outline,
				})
			--

			-- Functions 
				function cfg.toggle_menu(bool)
					window_outline.Visible = bool
					library:close_current_element(nil) 
				end
			-- 

			-- Keybind List
				-- Instances
					local keybind_list = library:create("Frame", {
						Parent = library.misc;
						Size = dim2(0, 120, 0, 0);
						Position = dim2(0, 100, 0, 500);
						BorderColor3 = rgb(0, 0, 0);
						BorderSizePixel = 0;
						AutomaticSize = Enum.AutomaticSize.Y;
						BackgroundColor3 = rgb(0, 0, 0)
					}); library:draggify(keybind_list)
					
					local inline = library:create("Frame", {
						Parent = keybind_list;
						Size = dim2(1, -2, 1, -2);
						Position = dim2(0, 1, 0, 1);
						BorderColor3 = rgb(0, 0, 0);
						BorderSizePixel = 0;
						AutomaticSize = Enum.AutomaticSize.Y;
						BackgroundColor3 = rgb(32, 32, 32)
					});
					
					local background = library:create("Frame", {
						Parent = inline;
						Size = dim2(1, -2, 1, -2);
						Position = dim2(0, 1, 0, 1);
						BorderColor3 = rgb(0, 0, 0);
						BorderSizePixel = 0;
						AutomaticSize = Enum.AutomaticSize.Y;
						BackgroundColor3 = rgb(8, 8, 8)
					});
					
					local text = library:create("TextLabel", {
						FontFace = library.font;
						TextColor3 = themes.preset.accent;
						BorderColor3 = rgb(0, 0, 0);
						Text = "Keybinds";
						Parent = background;
						BackgroundTransparency = 1;
						Position = dim2(0, 5, 0, 3);
						BorderSizePixel = 0;
						AutomaticSize = Enum.AutomaticSize.XY;
						TextSize = 12;
						BackgroundColor3 = rgb(74, 74, 74)
					}); library:apply_theme(text, "accent", "TextColor3")
					
					library:create("UIPadding", {
						PaddingBottom = dim(0, 5);
						Parent = background
					});
					
					local Keybinds = library:create("Frame", {
						Parent = background;
						Position = dim2(0, 0, 0, 24);
						BorderColor3 = rgb(0, 0, 0);
						Size = dim2(1, 0, 0, 0);
						BorderSizePixel = 0;
						BackgroundColor3 = rgb(255, 255, 255)
					}); library.keybind_list_parent = Keybinds
					
					library:create("UIListLayout", {
						Parent = Keybinds;
						Padding = dim(0, 10);
						SortOrder = Enum.SortOrder.LayoutOrder
					});
					
					library:create("UIPadding", {
						PaddingLeft = dim(0, 9);
						Parent = Keybinds
					});
					
					local _ = library:create("UICorner", {
						Parent = background;
						CornerRadius = dim(0, 4)
					});
					
					local accent = library:create("Frame", {
						Parent = background;
						Position = dim2(0, 1, 0, 20);
						BorderColor3 = rgb(0, 0, 0);
						Size = dim2(1, -2, 0, 1);
						BorderSizePixel = 0;
						BackgroundColor3 = themes.preset.accent
					}); library:apply_theme(accent, "accent", "BackgroundColor3")
					
					library:create("UIGradient", {
						Parent = accent;
						Transparency = numseq{numkey(0, 1), numkey(1, 0)}
					});
					
					library:create("UIPadding", {
						PaddingBottom = dim(0, 2);
						Parent = inline
					});
					
					local _ = library:create("UICorner", {
						Parent = inline;
						CornerRadius = dim(0, 4)
					});
					
					library:create("UIPadding", {
						PaddingBottom = dim(0, 2);
						Parent = keybind_list
					});
					
					local _ = library:create("UICorner", {
						Parent = keybind_list;
						CornerRadius = dim(0, 4)
					});
					
				
				-- 
			-- 

			return setmetatable(cfg, library)
		end 

		function library:tab(properties)
			local cfg = {
				name = properties.name or "Rage", 
				page_names = properties.sections or {"Aimbot", "Exploits", "Anti-Aim"},
				pages = {}, 
				current_page_data;
			} 

			-- Storing instances of the tab that are essential
			local tab_data = {
				accent; 
				page_directory;
				text;
				inline; 
				fillers = {}
			} 

			-- Instances
				-- Tab button
					local tab = library:create("Frame", {
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(0, 0, 0, 19),
						Position = dim2(0, 0, 0, 1),
						BorderSizePixel = 0,
						ZIndex = 5,
						BackgroundTransparency = 1,
						AutomaticSize = Enum.AutomaticSize.X,
						BackgroundColor3 = themes.preset.accent,
						Parent = self.button_holder,
						Name = "",
					}) library:apply_theme(tab, "accent", "BackgroundColor3")

					library:create("UICorner", {
						CornerRadius = UDim.new(0, 4),
						Parent = tab,
						Name = "",
					})

					local tab_inline = library:create("Frame", {
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, -2, 1, -2),
						Position = dim2(0, 0, 0, 1),
						BorderSizePixel = 0,
						ZIndex = 2,
						BackgroundTransparency = 1,
						AutomaticSize = Enum.AutomaticSize.X,
						BackgroundColor3 = rgb(11, 11, 11),
						Parent = tab,
						Name = "",
					})

					library:create("UICorner", {
						CornerRadius = UDim.new(0, 4),
						Parent = tab_inline,
						Name = "",
					})

					local tab_fill_left = library:create("Frame", {
						Position = dim2(0, 0, 0, 3),
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(0, 1, 1, -2),
						BackgroundTransparency = 1,
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(29, 29, 29),
						Parent = tab_inline,
						Name = "",
					})

					local tab_fill_bottom = library:create("Frame", {
						BorderColor3 = rgb(0, 0, 0),
						AnchorPoint = Vector2.new(0, 1),
						Position = dim2(0, 1, 1, 2),
						Size = dim2(1, -1000, 0, 5),
						BackgroundTransparency = 1,
						BorderSizePixel = 0,
						AutomaticSize = Enum.AutomaticSize.X,
						BackgroundColor3 = rgb(11, 11, 11),
						Parent = tab_inline,
						Name = "",
					})

					local tab_fill_right = library:create("Frame", {
						Position = dim2(1, 0, 0, 3),
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(0, 1, 1, -2),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(29, 29, 29),
						Parent = tab_inline,
						BackgroundTransparency = 1,
						Name = "",
					})

					local text_holder = library:create("Frame", {
						BorderColor3 = rgb(0, 0, 0),
						BorderSizePixel = 0,
						BackgroundTransparency = 1,
						Position = dim2(0, 0, 0, 1),
						Size = dim2(1, -2, 1, -2),
						ZIndex = 2,
						AutomaticSize = Enum.AutomaticSize.X,
						BackgroundColor3 = rgb(255, 255, 255),
						Parent = tab,
						Name = "",
					})

					local ui_title = library:create("TextButton", {
						FontFace = library.font,
						TextColor3 = rgb(74, 74, 74),
						BorderColor3 = rgb(0, 0, 0),
						Text = cfg.name,
						AutomaticSize = Enum.AutomaticSize.XY,
						Position = dim2(0, 4, 0, -2),
						BorderSizePixel = 0,
						BackgroundTransparency = 1,
						TextXAlignment = Enum.TextXAlignment.Left,
						RichText = true,
						ZIndex = 2,
						TextSize = 12,
						BackgroundColor3 = rgb(74, 74, 74),
						Parent = text_holder,
						Name = "",
					}) library:apply_theme(ui_title, "accent", "TextColor3")

					local UIPadding = library:create("UIPadding", {
						PaddingTop = UDim.new(0, 2),
						PaddingRight = UDim.new(0, 10),
						PaddingLeft = UDim.new(0, 7),
						Parent = text_holder,
						Name = "",
					})
				-- 

				-- Page directory 
					-- This holds the multi section inline alongside with the sections
					local page_directory = library:create("Frame", {
						BackgroundTransparency = 1,
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, 0, 1, 0),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(255, 255, 255),
						Parent = library.cache,
						Name = "",
						Visible = true,
					}); cfg.page_directory = page_directory;

					-- Multi section holder
						local multi_section_holder = library:create("Frame", {
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(0, 118, 1, 0),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(32, 32, 32),
							Parent = page_directory,
							Name = "",
						})

						local multi_section_inline = library:create("Frame", {
							Position = dim2(0, 1, 0, 1),
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(1, -2, 1, -2),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(8, 8, 8),
							Parent = multi_section_holder,
							Name = "",
						})

						library:create("UIPadding", {
							PaddingTop = UDim.new(0, 6),
							PaddingBottom = UDim.new(0, 6),
							PaddingRight = UDim.new(0, 5),
							PaddingLeft = UDim.new(0, 5),
							Parent = multi_section_inline,
							Name = "",
						})

						library:create("UIListLayout", {
							Padding = UDim.new(0, 3),
							SortOrder = Enum.SortOrder.LayoutOrder,
							Parent = multi_section_inline,
							Name = "",
						})
					--            
				-- 

				-- Page 
					for _, v in cfg.page_names do 
						local data = {} 

						-- Button Instances
							local tab_button = library:create("Frame", {
								BorderColor3 = rgb(0, 0, 0),
								Size = dim2(1, 0, 0, 18),
								BorderSizePixel = 0,
								BackgroundColor3 = rgb(255, 255, 255),
								Parent = multi_section_inline,
								Name = "",
							})

							local accent = library:create("Frame", {
								BorderColor3 = rgb(0, 0, 0),
								Size = dim2(0, 1, 1, 0),
								BorderSizePixel = 0,
								BackgroundColor3 = rgb(87, 86, 112),
								Parent = tab_button,
								Name = "",
							}) library:apply_theme(accent, "accent", "BackgroundColor3")

							local UIGradient = library:create("UIGradient", {
								Rotation = 180,
								Color = ColorSequence.new{
									ColorSequenceKeypoint.new(0, rgb(9, 9, 9)),
									ColorSequenceKeypoint.new(1, rgb(17, 17, 17))
								},
								Parent = tab_button,
								Name = "",
							})

							local ui_title = library:create("TextButton", {
								FontFace = library.font,
								TextColor3 = rgb(65, 65, 65),
								BorderColor3 = rgb(0, 0, 0),
								Text = v,
								Size = dim2(1, -7, 1, 0),
								BackgroundTransparency = 1,
								TextXAlignment = Enum.TextXAlignment.Left,
								Position = dim2(0, 7, 0, -2),
								BorderSizePixel = 0,
								TextSize = 12,
								BackgroundColor3 = rgb(255, 255, 255),
								Parent = tab_button,
								Name = "",
							}) library:apply_theme(ui_title, "accent", "TextColor3")
						--

						-- Page Directory 
							local page = library:create("Frame", {
								BackgroundTransparency = 1,
								Position = dim2(0, 128, 0, 0),
								BorderColor3 = rgb(0, 0, 0),
								Size = dim2(1, -129, 1, 0),
								BorderSizePixel = 0,
								Visible = true,
								BackgroundColor3 = rgb(32, 32, 32),
								Parent = library.cache,
								Name = "",
							})

							library:create("UIListLayout", {
								Padding = UDim.new(0, 3),
								SortOrder = Enum.SortOrder.LayoutOrder,
								VerticalFlex = Enum.UIFlexAlignment.Fill;
								HorizontalFlex = Enum.UIFlexAlignment.Fill;
								FillDirection = Enum.FillDirection.Horizontal;
								Parent = page,
								Name = "",
							})
						-- 

						data = {
							page = page,
							accent = accent,
							text = ui_title,
						}

						function data.open_page() 
							cfg.overwrite_selected_page(data)                              
						end 

						ui_title.MouseButton1Down:Connect(function()
							data.open_page() 
						end)

						cfg.pages[#cfg.pages + 1] = setmetatable(data, library)
					end 
				-- 

				tab_data = {
					page_directory = page_directory,
					accent = tab,
					text = ui_title, 
					inline = tab_inline,
					fillers = {
						tab_fill_left,
						tab_fill_bottom,
						tab_fill_right
					}
				} 
			-- 
			
			-- Functions 
				function cfg.overwrite_selected_page(new_data) -- This is for multi sections
					local data = cfg.current_page_data

					if data then 
						data.page.Visible = false 
						data.page.Parent = library.cache

						data.accent.BackgroundColor3 = rgb(87, 86, 112)
						data.text.TextColor3 = rgb(65, 65, 65)
					end 

					new_data.page.Parent = page_directory
					new_data.page.Visible = true

					new_data.accent.BackgroundColor3 = themes.preset.accent
					new_data.text.TextColor3 = themes.preset.accent

					cfg.current_page_data = new_data

					library:close_current_element(nil) 
				end 
				
				function cfg.open_tab() 
					local tab = self.selected_tab 

					if tab then 
						tab.accent.BackgroundTransparency = 1 

						tab.inline.BackgroundTransparency = 1 

						tab.text.TextColor3 = rgb(74, 74, 74)


						for _, filler in tab.fillers do 
							filler.BackgroundTransparency = 1
						end 

						tab.page_directory.Parent = library.cache
					end

					tab_data.accent.BackgroundTransparency = 0 

					tab_data.page_directory.Parent = self.page_holder 
					tab_data.inline.BackgroundTransparency = 0 
					tab_data.text.TextColor3 = themes.preset.accent

					for _, filler in tab_data.fillers do 
						filler.BackgroundTransparency = 0
					end

					self.selected_tab = tab_data
					library:close_current_element(nil)
				end 

				cfg.pages[1].open_page(true)
			--

			-- Connections 
				ui_title.MouseButton1Down:Connect(function()
					cfg.open_tab()
				end)

				if not self.selected_tab then 
					cfg.open_tab()
				end 
			-- 

			return unpack(cfg.pages)    
		end 

		function library:multi_section(properties)
			local cfg = {
				icons = properties.icons or {"104303181295923", "104303181295923"}, 
				sizes = properties.sizes or {dim2(0, 9, 0, 9), dim2(0, 9, 0, 9)},
				pages = {},
				selected_tab;
			} 

			local layout = self.page:FindFirstChildOfClass("UIListLayout")
			if layout then 
				layout:Destroy()
			end 

			-- Instances 
				-- Button holder
					local multi_section = library:create("Frame", {
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, 0, 0, 28),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(32, 32, 32),
						Parent = self.page,
						Name = "",
					})

					local multi_section_inline = library:create("Frame", {
						Position = dim2(0, 1, 0, 1),
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, -2, 1, -2),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(8, 8, 8),
						Parent = multi_section,
						Name = "",
					})

					library:create("UIListLayout", {
						FillDirection = Enum.FillDirection.Horizontal,
						HorizontalFlex = Enum.UIFlexAlignment.Fill,
						SortOrder = Enum.SortOrder.LayoutOrder,
						Parent = multi_section_inline,
						Name = "",
					})
				-- 

				-- Page holder
					local multi_section_pages = library:create("Frame", {
						BackgroundTransparency = 1,
						Position = dim2(0, 0, 0, 36),
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, 0, 1, -36),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(32, 32, 32),
						Parent = self.page,
						Name = "",
					})               
				--

			-- Pages init 
				for _, button in next, cfg.icons do 
					local data = {}; 
					
					-- Button 
						local button = library:create("TextButton", {
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(0, 0, 1, 0),
							Text = "",
							AutoButtonColor = false,
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = multi_section_inline,
							Name = "",
						})

						local image = library:create("ImageLabel", {
							ImageColor3 = rgb(74, 74, 74),
							BorderColor3 = rgb(0, 0, 0),
							AnchorPoint = Vector2.new(0.5, 0.5),
							Image = "rbxassetid://" .. cfg.icons[_],
							BackgroundTransparency = 1,
							Position = dim2(0.5, 0, 0.5, 0),
							Size = cfg.sizes[_],
							BorderSizePixel = 0,
							Parent = button,
							Name = "",
						}) library:apply_theme(image, "accent", "ImageColor3")

						local accent = library:create("Frame", {
							AnchorPoint = Vector2.new(0.5, 0),
							Position = dim2(0.5, 0, 1, -1),
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(0, 0, 0, 1),
							BorderSizePixel = 0,
							BackgroundColor3 = themes.preset.accent,
							Parent = button,
							Name = "",
						}) library:apply_theme(accent, "accent", "BackgroundColor3")

						local UIGradient = library:create("UIGradient", {
							Rotation = -90,
							Enabled = true,
							Transparency = NumberSequence.new{
								NumberSequenceKeypoint.new(0, 1),
								NumberSequenceKeypoint.new(1, 1)
							},
							Color = ColorSequence.new{
								ColorSequenceKeypoint.new(0, rgb(17, 17, 17)),
								ColorSequenceKeypoint.new(1, rgb(8, 8, 8))
							},
							Parent = button,
							Name = "",
						})
					-- 

					-- Page 
						local page = library:create("Frame", {
							BackgroundTransparency = 1,
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(1, 0, 1, 0),
							BorderSizePixel = 0,
							Visible = true, 
							BackgroundColor3 = rgb(32, 32, 32),
							Parent = library.cache,
							Name = "",
						})

						library:create("UIListLayout", {
							FillDirection = Enum.FillDirection.Horizontal,
							HorizontalFlex = Enum.UIFlexAlignment.Fill,
							Padding = UDim.new(0, 8),
							SortOrder = Enum.SortOrder.LayoutOrder,
							VerticalFlex = Enum.UIFlexAlignment.Fill,
							Parent = page,
							Name = "",
						})
						--

						data = {
							image = image,
							gradient = UIGradient,
							accent = accent,
							page = page
						}
					-- 

					-- Functions 
						function data.open_tab() 
							local old_data = cfg.selected_tab
							if old_data then 
								old_data.image.ImageColor3 = rgb(74, 74, 74)

								library:tween(old_data.accent, {
									Size = dim2(0, 0, 0, 1)
								})

								old_data.page.Parent = library.cache 
								old_data.gradient.Transparency = NumberSequence.new{
									NumberSequenceKeypoint.new(0, 1),
									NumberSequenceKeypoint.new(1, 1)
								}
							end 

							image.ImageColor3 = themes.preset.accent

							library:tween(accent, {
								Size = dim2(1, 0, 0, 1)
							})

							page.Parent = multi_section_pages

							UIGradient.Transparency = NumberSequence.new{
								NumberSequenceKeypoint.new(0, 0),
								NumberSequenceKeypoint.new(1, 0)
							}

							cfg.selected_tab = data
							library:close_current_element(nil)
						end 
					-- 

					-- Connections
						button.MouseButton1Click:Connect(function()
							data.open_tab()
						end)
					--

					cfg.pages[#cfg.pages + 1] = setmetatable(data, library)
				end 

				cfg.pages[1].open_tab()
			-- 

			return unpack(cfg.pages)
		end 

		function library:column()   
			local cfg = {
				parent;
			}

			local section = library:create("Frame", {
				BackgroundTransparency = 1,
				BorderColor3 = rgb(0, 0, 0),
				BorderSizePixel = 0,
				Size = dim2(1, 0, 1, 0),
				BackgroundColor3 = rgb(32, 32, 32),
				Parent = self.page,
				Name = "",
			}); cfg.parent = section;

			return setmetatable(cfg, library)             
		end 

		function library:section(properties)
			local cfg = {
				name = properties.name or properties.Name or "Aimbot", 
				size = properties.size or properties.Size or dim2(1, 0, 1, 0)
			}   

			--[[
				Hello, you are probably reading this wanting seperate sections in one column?
				Ive got you covered.
				Simply just use the size property like this:

				local column = tab:column() 

				-- Leave the first section to just be whatever size you want it to be out of 1
				column:section({
					size = dim2(0, 0, 0.4, 0) -- this will take 0.4 of the section
				}) 

				-- to fill the rest you do 1 - the other fill to get you 0.6
				-- However we need to add -8 into the dim2 offset in order to account for the list layout
				column:section({
					size = dim2(0, 0, 0.6, -8) 
				}) 
			]]

			local section = library:create("Frame", {
				BorderColor3 = rgb(0, 0, 0),
				BorderSizePixel = 0,
				Size = cfg.size,
				BackgroundColor3 = rgb(32, 32, 32),
				Parent = self.parent,
				Name = "",
			})

			local section_inline = library:create("Frame", {
				Position = dim2(0, 1, 0, 1),
				BorderColor3 = rgb(0, 0, 0),
				Size = dim2(1, -2, 1, -2),
				BorderSizePixel = 0,
				BackgroundColor3 = rgb(8, 8, 8),
				Parent = section,
				Name = "",
			})

			local top = library:create("Frame", {
				BorderColor3 = rgb(0, 0, 0),
				Size = dim2(1, 0, 0, 22),
				BorderSizePixel = 0,
				BackgroundColor3 = rgb(255, 255, 255),
				Parent = section_inline,
				Name = "",
			})

			library:create("UIGradient", {
				Rotation = 90,
				Color = ColorSequence.new{
					ColorSequenceKeypoint.new(0, rgb(16, 16, 16)),
					ColorSequenceKeypoint.new(1, rgb(8, 8, 8))
				},
				Parent = top,
				Name = "",
			})

			local fill = library:create("Frame", {
				Position = dim2(0, 1, 1, -1),
				BorderColor3 = rgb(0, 0, 0),
				Size = dim2(1, -2, 0, 1),
				BorderSizePixel = 0,
				BackgroundColor3 = rgb(22, 22, 22),
				Parent = top,
				Name = "",
			})

			library:create("UIGradient", {
				Transparency = NumberSequence.new{
					NumberSequenceKeypoint.new(0, 0),
					NumberSequenceKeypoint.new(0.5, 1),
					NumberSequenceKeypoint.new(1, 0)
				},
				Parent = fill,
				Name = "",
			})

			local section_title = library:create("TextLabel", {
				FontFace = library.font,
				TextColor3 = rgb(74, 74, 74),
				BorderColor3 = rgb(0, 0, 0),
				Text = cfg.name,
				Size = dim2(1, -7, 1, 0),
				BackgroundTransparency = 1,
				TextXAlignment = Enum.TextXAlignment.Left,
				Position = dim2(0, 7, 0, -2),
				BorderSizePixel = 0,
				TextSize = 12,
				BackgroundColor3 = rgb(255, 255, 255),
				Parent = top,
				Name = "",
			})

			local ScrollingFrame = library:create("ScrollingFrame", {
				ScrollBarImageColor3 = rgb(31, 31, 31),
				Active = true,
				AutomaticCanvasSize = Enum.AutomaticSize.Y,
				ScrollBarThickness = 0,
				Size = dim2(1, -1, 1, -23),
				BackgroundTransparency = 1.1299999952316284,
				Position = dim2(0, 0, 0, 22),
				BackgroundColor3 = rgb(255, 255, 255),
				BorderColor3 = rgb(0, 0, 0),
				BorderSizePixel = 0,
				CanvasSize = dim2(0, 0, 0, 0),
				Parent = section_inline,
				Name = "",
			})

			local elements = library:create("Frame", {
				Position = dim2(0, 16, 0, 22),
				BorderColor3 = rgb(0, 0, 0),
				Size = dim2(1, -24, 0, 0),
				BorderSizePixel = 0,
				BackgroundColor3 = rgb(255, 255, 255),
				Parent = ScrollingFrame,
				Name = "",
			}); cfg.elements = elements;

			local UIListLayout = library:create("UIListLayout", {
				Padding = UDim.new(0, 9),
				HorizontalAlignment = Enum.HorizontalAlignment.Center,
				SortOrder = Enum.SortOrder.LayoutOrder,
				Parent = elements,
				Name = "",
			})

			local UIPadding = library:create("UIPadding", {
				PaddingBottom = UDim.new(0, 10),
				Parent = elements,
				Name = "",
			})


			-- Connections 
				ScrollingFrame.MouseEnter:Connect(function()
					library:tween(ScrollingFrame, {ScrollBarThickness = 3})
				end)

				ScrollingFrame.MouseLeave:Connect(function()
					library:tween(ScrollingFrame, {ScrollBarThickness = 0})
				end)

				ScrollingFrame:GetPropertyChangedSignal("CanvasPosition"):Connect(function()
					library:close_current_element()
				end)
			-- 

			return setmetatable(cfg, library)
		end 

		-- Elements
			function library:toggle(options) 
				local cfg = {
					enabled = options.enabled or nil,
					name = options.name or "Toggle",
					flag = options.flag or options.name or "nigga",

					default = options.default or false,
					folding = options.folding or false, 
					callback = options.callback or function() end,
				}

				-- Instances 
					local object = library:create("TextButton", {
						FontFace = library.font,
						TextColor3 = rgb(158, 158, 158),
						BorderColor3 = rgb(0, 0, 0),
						Text = "",
						Size = dim2(1, -26, 0, 7),
						BorderSizePixel = 0,
						BackgroundTransparency = 1,
						TextXAlignment = Enum.TextXAlignment.Left,
						TextYAlignment = Enum.TextYAlignment.Top,
						TextSize = 12,
						BackgroundColor3 = rgb(255, 255, 255),
						Parent = self.elements,
						Name = "",
					})

					local right_components = library:create("Frame", {
						Position = dim2(1, 13, 0, 1),
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(0, 0, 0, 9),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(255, 255, 255),
						Parent = object,
						Name = "",
					}) cfg.right_components = right_components

					local list = library:create("UIListLayout", {
						VerticalAlignment = Enum.VerticalAlignment.Center,
						FillDirection = Enum.FillDirection.Horizontal,
						HorizontalAlignment = Enum.HorizontalAlignment.Right,
						Padding = UDim.new(0, 4),
						SortOrder = Enum.SortOrder.LayoutOrder,
						Parent = right_components,
						Name = "",
					})

					local left_holder = library:create("Frame", {
						BackgroundTransparency = 1,
						Position = dim2(0, -15, 0, 1),
						BorderColor3 = rgb(0, 0, 0),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(255, 255, 255),
						Parent = object,
						Name = "",
					})

					local toggle = library:create("Frame", {
						Position = dim2(0, -15, 0, 1),
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(0, 8, 0, 8),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(0, 0, 0),
						Parent = left_holder,
						Name = "",
					})

					local toggle_inline = library:create("Frame", {
						Position = dim2(0, 1, 0, 1),
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, -2, 1, -2),
						BorderSizePixel = 0,
						BackgroundColor3 = rgb(32, 32, 32),
						Parent = toggle,
						Name = "",
					})

					local toggle_accent = library:create("Frame", {
						BorderColor3 = rgb(0, 0, 0),
						Size = dim2(1, 0, 1, 0),
						Visible = true,
						BackgroundTransparency = 1,
						BorderSizePixel = 0,
						BackgroundColor3 = themes.preset.accent,
						Parent = toggle_inline,
						Name = "",
					}) library:apply_theme(toggle_accent, "accent", "BackgroundColor3")

					local toggle_gradient = library:create("UIGradient", {
						Enabled = true,
						Rotation = 90,
						Color = ColorSequence.new{
							ColorSequenceKeypoint.new(0, rgb(255, 255, 255)),
							ColorSequenceKeypoint.new(1, rgb(126, 126, 126))
						},
						Parent = toggle_accent,
						Name = "",
					})

					library:create("UIListLayout", {
						FillDirection = Enum.FillDirection.Horizontal,
						Padding = UDim.new(0, 7),
						SortOrder = Enum.SortOrder.LayoutOrder,
						Parent = left_holder,
						Name = "",
					})

					local text = library:create("TextLabel", {
						FontFace = library.font,
						TextColor3 = rgb(74, 74, 74),
						BorderColor3 = rgb(0, 0, 0),
						Text = cfg.name,
						Size = dim2(0, 0, 0, 5),
						Position = dim2(0, 4, 0, -2),
						BorderSizePixel = 0,
						BackgroundTransparency = 1,
						TextXAlignment = Enum.TextXAlignment.Left,
						RichText = true,
						AutomaticSize = Enum.AutomaticSize.X,
						TextSize = 12,
						BackgroundColor3 = rgb(74, 74, 74),
						Parent = left_holder,
						Name = "",
					})

					if cfg.folding then 
						local bottom_components = library:create("Frame", {
							Position = dim2(0, 0, 0, 17),
							BorderColor3 = rgb(0, 0, 0),
							Size = dim2(1, 26, 0, 0),
							BorderSizePixel = 0,
							BackgroundColor3 = rgb(255, 255, 255),
							Parent = library.cache,
							Name = "",
						})

						library:create("UIListLayout", {
							Padding = UDim.new(0, 10),
							SortOrder = Enum.SortOrder.LayoutOrder,
							Parent = bottom_components,
							Name = "",
						})
					end
				-- 

				-- Functions 
					function cfg.set(bool) 
						library:tween(toggle_accent, {
							BackgroundTransparency = bool and 0 or 1, 
						})

						library:tween(text, {
							TextColor3 = bool and rgb(221, 221, 221) or rgb(74, 74, 74), 
						})

						cfg.callback(bool)

						if cfg.folding then 
							cfg.background.Parent = object
						end 

						cfg.enabled = bool

						flags[cfg.flag] = bool
					end 

					cfg.set(cfg.default)

					config_flags[cfg.flag] = cfg.set
				-- 

				-- Connections
					object.MouseButton1Click:Connect(function()
						cfg.enabled = not cfg.enabled 
						cfg.set(cfg.enabled)
					end)

					object.MouseEnter:Connect(function()
						if cfg.enabled then 
							return 
						end 

						library:tween(text, {
							TextColor3 = rgb(150, 150, 150), 
						})

						library:tween(toggle_accent, {
							BackgroundTransparency = 0.5, 
						})
					end)

					object.MouseLeave:Connect(function()
						if cfg.enabled then 
							return 
						end 

						library:tween(text, {
							TextColor3 = rgb(74, 74, 74), 
						})

						library:tween(toggle_accent, {
							BackgroundTransparency = 1, 
						})
					end)
				-- 

				return setmetatable(cfg, library)
			end 

